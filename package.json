{
  "name": "highlight.js-js-styles",
  "version": "1.0.0",
  "description": "all of highlight.js css style sheets as javascript objects",
  "main": "dist/styles/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "watch": "babel src --watch --out-dir dist",
    "build-styles": "node ./lib/build-stylesheets.js",
    "prepublish": "npm run build-styles && npm run build"
  },
  "keywords": [
    "highlight.js",
    "highlight",
    "code",
    "code-highlighting"
  ],
  "author": "Conor Hastings",
  "license": "MIT",
  "devDependencies": {
    "css": "^2.2.1",
    "highlight.js": "^9.7.0",
    "to-camel-case": "^1.0.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0"
  }
}
